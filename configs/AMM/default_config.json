{
  "train_params": {
    "num_iterations": 5000,
    "learning_rate": 0.01,
    "loss_function": "CrossEntropyLoss",
    "test_interval": 100,
    "batch_size": 10000
  },
  "data_params": {
    "max_samples_per_class": 30000,
    "loaded_test_samples": 50000,
