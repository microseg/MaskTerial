    "DBSCAN_eps": 0.1,
    "use_Nearest_Neighbors": true,
    "neighbors": 25
  },
  "model_arch": {
    "num_classes": 2,
    "input_dim": 3,
    "embedding_dim": 16,
    "depth": 4,
    "spec_coeff": 0.5,
    "n_power_iterations": 5,
    "spectral_normalization": true,
    "dropout_rate": 0.1
  }
}
